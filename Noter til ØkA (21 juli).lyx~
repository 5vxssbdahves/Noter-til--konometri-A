\begin_layout Subsection
Betingede forventninger
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\]

\end_inset


\end_layout

\begin_layout Standard

\end_layout

\begin_layout Paragraph
Litteraturhenvisninger
\end_layout

\begin_layout Standard
